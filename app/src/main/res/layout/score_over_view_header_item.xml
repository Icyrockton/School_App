<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginLeft="@dimen/dp_15"
    android:layout_marginRight="@dimen/dp_15"
    android:layout_marginTop="@dimen/dp_15"
    android:clickable="true"
    android:id="@+id/score_over_view_header_layout"
    app:cardBackgroundColor="?colorPrimaryDark"
    android:elevation="@dimen/dp_10"
    android:layout_height="250dp">

    <com.db.williamchart.view.HorizontalBarChartView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/dp_20"
        android:id="@+id/score_over_view_bar_chart"
        app:chart_axis="y"
        app:chart_barsColor="#fff"
        app:chart_barsRadius="6dp"
        app:chart_labelsColor="#fff"
        app:chart_labelsSize="15sp"
        app:chart_spacing="20dp"/>



</com.google.android.material.card.MaterialCardView>